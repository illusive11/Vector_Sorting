void vec_sort(std::vector<int>& vec) {
	for (int i = 0; i < vec.size(); i++) {
		for (int a = 0; a < vec.size() - 1; a++) {
			if (vec[a] > vec[a + 1]) {
				std::swap(vec[a], vec[a + 1]);
			}
		}
	}

	for (int b = 0; b < vec.size(); b++) {
		std::cout << vec[b] << "   =   " << &vec[b] << std::endl;
	}
}
